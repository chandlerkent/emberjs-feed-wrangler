{{#if isLoading}}
	<br />
	<br />
	<br />
	<br />
	<center>Loading...</center>
{{else}}
	{{#if loadError}}
		<br />
		<br />
		<br />
		<br />
		<center>{{loadError}}</center>    
	{{else}}
		{{#each model}}
			{{view App.FeedItemView}}
		{{else}}
			<br />
			<br />
			<br />
			<br />
			<center>No articles found.</center>
		{{/each}}
	{{/if}}
{{/if}}